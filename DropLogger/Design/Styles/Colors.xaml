<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="LightGrayColor">#C3C3C3</Color>
    <SolidColorBrush x:Key="LightGrayColorBrush" Color="{StaticResource LightGrayColor}"/>

    <Color x:Key="MediumGrayColor">#B6C9D2</Color>
    <SolidColorBrush x:Key="MediumGrayColorBrush" Color="{StaticResource MediumGrayColor}"/>

    <Color x:Key="PrussianBlueColor">#0B3954</Color>
    <SolidColorBrush x:Key="PrussianBlueColorBrush" Color="{StaticResource PrussianBlueColor}" />

    <Color x:Key="GunMetalColor">#072736</Color>
    <SolidColorBrush x:Key="GunMetalColorBrush" Color="{StaticResource GunMetalColor}"/>
    
    <Color x:Key="RichBlackFOGRAColor">#051924</Color>
    <SolidColorBrush x:Key="RichBlackFOGRAColorBrush" Color="{StaticResource RichBlackFOGRAColor}" />
</ResourceDictionary>